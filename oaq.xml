<?xml version="1.0"?>
<mavlink>
  <version>3</version>

  <messages>
    <message id="42001" name="OAQ_MOCAP_STATE">
      <description>OAQ custom mocap state message</description>

      <field type="uint32_t" name="time_ms">Timestamp (ms)</field>

      <field type="float" name="x">Position X (m)</field>
      <field type="float" name="y">Position Y (m)</field>
      <field type="float" name="z">Position Z (m)</field>

      <field type="float" name="yaw">Yaw angle (rad)</field>

      <field type="float" name="vx">Velocity X (m/s)</field>
      <field type="float" name="vy">Velocity Y (m/s)</field>
      <field type="float" name="vz">Velocity Z (m/s)</field>

      <field type="uint8_t" name="status">0:INVALID, 1:OK</field>
    </message>
  </messages>
</mavlink>
